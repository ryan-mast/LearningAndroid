<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.learning.ryanmast.demoapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HelloWorld">
            android:name="com.learning.ryanmast.demoapp.HelloWorld"
            android:label="@string/title_activity_hello_world"
            android:parentActivityName="com.learning.ryanmast.demoapp.LaunchActivity >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.learning.ryanmast.demoapp.LaunchActivity" />
        </activity>
        <activity android:name=".DisplayHelloWorldActivity">
            android:name="com.learning.ryanmast.demoapp.DisplayHelloWorldActivity"
            android:label="@string/title_activity_display_hello_world_message"
            android:parentActivityName="com.learning.ryanmast.demoapp.HelloWorld >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.learning.ryanmast.demoapp.HelloWorld" />
        </activity>
        <activity android:name=".LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>